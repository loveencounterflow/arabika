
############################################################################################################
π                         = require 'coffeenode-packrattle'
NEW                       = require './new'



#-----------------------------------------------------------------------------------------------------------
@digits = ( π.regex /[0-9]+/ )
  .onMatch ( match ) => NEW.literal match[ 0 ], match[ 0 ]

#-----------------------------------------------------------------------------------------------------------
# @integer = ( π.regex /[0-9]+/ )
#   .onMatch ( match ) => NEW.literal match[ 0 ], parseInt match[ 0 ], 10

#-----------------------------------------------------------------------------------------------------------
@number = π.alt @digits

#-----------------------------------------------------------------------------------------------------------
@_tests = ->
  assert  = require 'assert'
  #---------------------------------------------------------------------------------------------------------
  @digits = =>
    assert.deepEqual 1, 1



