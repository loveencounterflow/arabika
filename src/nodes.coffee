

############################################################################################################
MULTIMIX                  = require 'coffeenode-multimix'



#-----------------------------------------------------------------------------------------------------------
@binary_expression = ( operator, left, right ) ->
  R =
    type:     'BinaryExpression'
    operator: operator
    left:     left
    right:    right
  return R

#-----------------------------------------------------------------------------------------------------------
@literal = ( raw, value ) ->
  R =
    type:     'Literal'
    value:    value
    raw:      raw
  return R


############################################################################################################
MULTIMIX.bundle @


